	<queryString>
		<![CDATA[SELECT
 sum(public.recettepanifie.valeur) as valeur,
  public.recettepanifie.annees,
 SUM(public.recetteprogramme.coutglobal) as coutglobal,
 public.recetteprogramme.sourcefinancements AS scprog,
  public.recettepanifie.idannee,
  public.recettepanifie.soucesfinancements AS scplanif
FROM
  public.recettepanifie
  LEFT OUTER JOIN public.recetteprogramme ON (public.recettepanifie.idannee = public.recetteprogramme.idannee)
  AND (public.recettepanifie.idsourcefi = public.recetteprogramme.idsourcefi)
WHERE
  public.recettepanifie.iddistrict =$P{iddistrict}
  GROUP by public.recettepanifie.soucesfinancements,  public.recettepanifie.soucesfinancements,public.recettepanifie.valeur,
  public.recettepanifie.annees,public.recetteprogramme.sourcefinancements,
 public.recettepanifie.idannee
ORDER BY
  public.recetteprogramme.sourcefinancements
  ]]>
	</queryString>
